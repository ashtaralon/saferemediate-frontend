# AWS Credentials for Real Data & Remediation
# Copy this file to .env.local and fill in your values

# AWS Credentials (use IAM user with appropriate permissions)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=eu-west-1

# Or use AWS SSO/Profile (recommended)
# AWS_PROFILE=your-profile-name

# Backend URL (optional - falls back to direct AWS calls)
NEXT_PUBLIC_BACKEND_URL=https://saferemediate-backend.onrender.com

# Neo4j (optional - for graph features)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password
