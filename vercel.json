{
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    },
    "app/api/proxy/cloudtrail/**/*.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
